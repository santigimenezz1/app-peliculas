{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": ".next/**/*",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ],
    "build": {
      "env": {
        "CI": "true"
      },
      "ignore": [
        "Prevent writing to file that only differs in casing or query string from already written file."
      ]
    }
  }
  